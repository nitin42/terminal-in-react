{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "transform-decorators-legacy",
    ["transform-es2015-modules-umd", {
      "globals": {
        "react": "React"
      }
    }]
  ],
  "env": {
    "production": {
      "presets": [
        [
          "babili",
          {
            "evaluate": false,
            "mangle": false
          }
        ]
      ]
    }
  }
}
